[Problem]
Name = rb_single

[TimeLoop]
TEnd = 1.e9 # 1e9 # [s]
DtInitial = 1 # [s]

[RootSystem.Grid]
File =  singleroot
InitialT = 50 # days

[RootSystem.Conductivity]
Kx = 4.32e-2 # axial conductivity [cm^4/hPa/day] 
Kr = 1.73e-4 # radial conductivity [cm/hPa/day] 

[RootSystem.Collar]
P = -1000 # cm pressure head

[Soil.IC]
P = -200 # cm pressure head

[Vtk]
AddProcessRank = "false"
AddVelocity = "true"
